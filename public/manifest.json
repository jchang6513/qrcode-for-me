{
  "manifest_version": 3,
  "name": "Qrcode For Me",
  "version": "0.1.0",
  "description": "Generate QR code for any web page with ease using this Chrome extension.",
  "icons": {
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_title": "Qrcode For Me",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
